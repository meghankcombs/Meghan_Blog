@model Meghan_Blog.Models.EmailModel

    <div class="e-divider-2"></div>
    <!-- Page Content -->
    <section id="articles" class="">
        <div class="container">
            <div class="row">
                <article id="01" class="page-block col-md-12 page hentry">
                    <div class="page-detail text-center">
                        <h2 class="entry-title page-title"><a href="#!">Contact me</a></h2>
                    </div>
                    <div class="text-center"><div class="ornament-divider-fullwidth"><i class="flaticon-art-3"></i></div></div>
                    <div class="inner-content"></div>
                </article>
            </div>
        </div>
    </section>
    <!-- /Page Content -->
    <!-- Feedback Block -->
    <section id="feedback-block" class="e-block-null text-center">
        <div class="container padding-side-60">
            <div class="row">
                <div class="col-md-12 e-centered">
                    <div class="e-divider-2"></div>
                    <div id="form-messages"></div>
                    @using (Html.BeginForm("Contact", "Home", FormMethod.Post))
                    {
                        @Html.AntiForgeryToken()
                    <div class="field">
                        @*<input type="text" id="name" name="name" placeholder="Name" required>*@
                        @Html.TextBoxFor(m => m.FromName, new { placeholder = "Name" })
                    </div>
                        <div class="e-divider-1"></div>
                        <div class="field">
                            @*<input type="email" id="email" name="email" placeholder="Email" required>*@
                            @Html.TextBoxFor(m => m.FromEmail, new { placeholder = "Email" })
                        </div>
                        <div class="e-divider-1"></div>
                        <div class="field">
                            @*<textarea id="message" name="message" placeholder="Message" rows="7" cols="30" required></textarea>*@
                            @Html.TextAreaFor(m => m.Body, new { rows = 7, cols = 30, placeholder = "Your Message" })
                        </div>
                        <div class="e-divider-2"></div>
                        <div class="field text-center">
                            <button type="submit" class="btn btn-lg btn-blk">Send Message</button>
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>
    <!-- /Feedback Block -->
    <div class="e-divider-2"></div>
    <!--Contact Data-->
    <section id="share" class="e-block-null">
        <div class="container text-center">
            <div class="share-tag-box">
                <div class="row">
                    <div class="col-md-12">
                        <h4 class="darkrose lined">Reach Out via Social Media</h4>
                        <ul class="list-inline social share">
                            <!-- Social Share -->
                            <li>
                                <a href="https://www.linkedin.com/in/meghankcombs/" class="rst-icon-linkedin"><i class="fa fa-linkedin"></i></a>
                            </li>
                            <li>
                                <a class="rst-icon-pinterest" href="https://github.com/meghankcombs"><i class="fa fa-github"></i></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="e-divider-4"></div>
    </section>
    <!--/Contact Data-->
    
    <div class="text-center"><div class="ornament-divider-fullwidth"><i class="flaticon-art-3"></i></div></div>
    <div class="e-divider-4"></div>
